{
    "name": "Sort By: Oldest",
    "version": "1.1.0",
    "manifest_version": 3,
    "permissions": ["tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
      },
    "description" : "Returns YouYube Sort by Oldest",
    "content_scripts" : [
       {
        "matches": ["https://*.youtube.com/*"],
        "js" : ["contentScript.js"]
       }

    ]
}
