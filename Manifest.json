{
    "name": "Sort By: Oldest",
    "version": "1.1.0",
    "manifest_version": 2,
    "permissions": ["tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "scripts": ["background.js"]
      },
    "description" : "Return YouTube Sort by Oldest",
    "content_scripts" : [
       {
        "matches": ["https://*.youtube.com/*"],
        "js" : ["contentScript.js"]
       }

    ]
}
