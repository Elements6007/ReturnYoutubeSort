{
    "name": "returnyoutubesortbyoldest",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": ["storage","tabs"],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
      },
    "description" : "returns youtube sort by oldest",
    "content_scripts" : [
       {
        "matches": ["https://*.youtube.com/*"],
        "js" : ["contentScript.js"]
       }

    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/button.jpg"
        ],
        "matches": ["https://*.youtube.com/*"]
      }
    ]
}
